server {
    listen              443 ssl;
    ssl_certificate     /etc/nginx/me.crt;
    ssl_certificate_key /etc/nginx/me.key;
    server_name         git.mokoupi.local;

    location / {
        proxy_pass      http://192.168.255.6:3000;
    }

    access_log          /tmp/nginx-8180-access.log;
}
